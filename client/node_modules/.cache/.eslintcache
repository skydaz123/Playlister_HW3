) {\r\n        this.store.deleteSong(this.songIndex);\r\n    }\r\n    \r\n    undoTransaction() {\r\n        this.store.insertSong(this.songIndex, this.originalSong);\r\n    }\r\n}","C:\\Users\\haodu\\OneDrive\\Desktop\\hw3\\d\\client\\src\\transactions\\EditSong_Transaction.js",["165","166"],"import jsTPS_Transaction from \"../common/jsTPS.js\"\r\nimport { useContext } from 'react'\r\nimport { GlobalStoreContext } from '../store'\r\n\r\nexport default class EditSong_Transaction extends jsTPS_Transaction {\r\n    constructor(initStore, initSongIndex, initOriginalSong, initTitle, initArtist, initYouTubeId) {\r\n        super();\r\n        this.store = initStore;\r\n        this.songIndex = initSongIndex;\r\n        this.originalSong = initOriginalSong;\r\n        this.title = initTitle;\r\n        this.artist = initArtist;\r\n        this.youTubeId = initYouTubeId;\r\n    }\r\n\r\n    doTransaction() {\r\n        this.store.editSong(this.songIndex, this.title, this.artist, this.youTubeId);\r\n    }\r\n    \r\n    undoTransaction() {\r\n        this.store.reverseEditSong(this.songIndex, this.originalSong);\r\n    }\r\n}","C:\\Users\\haodu\\OneDrive\\Desktop\\hw3\\d\\client\\src\\api\\index.js",[],"C:\\Users\\haodu\\OneDrive\\Desktop\\hw3\\d\\client\\src\\components\\EditToolbar.js",["167","168"],"C:\\Users\\haodu\\OneDrive\\Desktop\\hw3\\d\\client\\src\\components\\ListCard.js",[],"C:\\Users\\haodu\\OneDrive\\Desktop\\hw3\\d\\client\\src\\components\\ListSelector.js",["169"],"C:\\Users\\haodu\\OneDrive\\Desktop\\hw3\\d\\client\\src\\components\\Banner.js",[],"C:\\Users\\haodu\\OneDrive\\Desktop\\hw3\\d\\client\\src\\components\\Statusbar.js",[],"C:\\Users\\haodu\\OneDrive\\Desktop\\hw3\\d\\client\\src\\components\\SongCard.js",["170"],"import React, { useContext, useState } from 'react'\r\nimport { GlobalStoreContext } from '../store'\r\n\r\nfunction SongCard(props) {\r\n    const { song, index } = props;\r\n    const { store } = useContext(GlobalStoreContext);\r\n   